{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "prettier/prettier": "warn"
    }
}